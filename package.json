{
  "name": "tuktuk-chart-plotter",
  "version": "0.0.18",
  "description": "Tuktuk chart plotter with SignalK and MBTiles support",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "lint": "eslint --ext=js src/",
    "lint-fix": "eslint --ext=js src/ --fix",
    "test": "npm run lint",
    "bundle": "npm run lint && npm run bundle:css && NODE_ENV=production npm run bundle:js",
    "bundle:js": "rm ./public/bundle.js || true && ./node_modules/.bin/webpack --hide-modules --bail -p --config=webpack.bundle.js",
    "bundle:css": "rm ./public/bundle.css || true && ./node_modules/.bin/lessc src/styles/app.less > .bundle.css && node_modules/postcss-cli/bin/postcss --use autoprefixer --replace .bundle.css && mv .bundle.css public/bundle.css",
    "watch:js": "./node_modules/.bin/webpack --hide-modules --watch --config=webpack.bundle.js",
    "watch:css": "npm run bundle:css && watch-run -p 'src/styles/*.less' 'npm run bundle:css'",
    "watch:server": "nodemon --ext js,css,json --watch src index.js",
    "watch": "npm run watch:js & npm run watch:css & npm run watch:server & wait",
    "prepublishOnly": "npm run bundle"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vokkim/tuktuk-chart-plotter.git"
  },
  "author": "Mikko Vesikkala",
  "keywords": [
    "signalk",
    "mbtiles",
    "chart",
    "plotter",
    "marine",
    "signalk-webapp"
  ],
  "dependencies": {},
  "devDependencies": {
    "@mapbox/mbtiles": "0.9.0",
    "@signalk/client": "0.2.1",
    "autoprefixer": "8.2.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-react-transform": "3.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "bacon.atom": "5.0.5",
    "baconjs": "2.0.5",
    "baret": "1.1.1",
    "classnames": "2.2.5",
    "compression": "1.7.2",
    "eslint": "^4.19.1",
    "eslint-plugin-es5": "1.2.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-restricted-globals": "^0.1.1",
    "express": "4.16.3",
    "geolib": "2.0.24",
    "leaflet": "1.3.1",
    "leaflet-rotatedmarker": "0.2.0",
    "less": "3.0.1",
    "lodash": "4.17.5",
    "nodemon": "1.17.2",
    "numeral": "2.0.6",
    "partial.lenses": "13.7.4",
    "postcss-cli": "5.0.0",
    "prettier": "^1.12.1",
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "react-drag-sortable": "1.0.6",
    "request": "2.85.0",
    "screenfull": "3.3.2",
    "store": "2.0.12",
    "watch-run": "1.2.5",
    "webpack": "4.4.1",
    "webpack-cli": "2.0.13",
    "whatwg-fetch": "2.0.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "bracketSpacing": false
  }
}
